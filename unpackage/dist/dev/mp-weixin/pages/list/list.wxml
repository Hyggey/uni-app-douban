<view class="listContainer"><swiper indicator-dots="{{true}}" indicator-active-color="#e43a3d" autoplay="{{false}}" interval="{{3000}}" duration="{{1000}}"><block wx:for="{{noPlayingList.subjects}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="swiper-item"><image src="{{item.images.large}}" mode="widthFix" data-event-opts="{{[['tap',[['toDetails',['$0'],[[['noPlayingList.subjects','',index,'id']]]]]]]}}" bindtap="__e"></image></view></swiper-item></block></swiper><view class="wrap"><view class="title">{{noPlayingList.title}}<text data-event-opts="{{[['tap',[['toClass',[3]]]]]}}" bindtap="__e">更多></text></view><scroll-view class="scroll-wrap" scroll-x="{{true}}"><block wx:for="{{noPlayingList.subjects}}" wx:for-item="it" wx:for-index="idx" wx:key="idx"><view class="slider"><image src="{{it.images.small}}" mode data-event-opts="{{[['tap',[['toDetails',['$0'],[[['noPlayingList.subjects','',idx,'id']]]]]]]}}" bindtap="__e"></image><view class="title2">{{it.title}}</view></view></block></scroll-view></view><block wx:for="{{moveList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="wrap"><view class="title">{{item.title}}<text data-event-opts="{{[['tap',[['toClass',[index]]]]]}}" bindtap="__e">更多></text></view><scroll-view class="scroll-wrap" scroll-x="{{true}}"><block wx:for="{{item.subjects}}" wx:for-item="it" wx:for-index="idx" wx:key="idx"><view class="slider"><image src="{{it.images.small}}" mode data-event-opts="{{[['tap',[['toDetails',['$0'],[[['moveList','',index],['subjects','',idx,'id']]]]]]]}}" bindtap="__e"></image><view class="title2">{{it.title}}</view></view></block></scroll-view></view></block></view>